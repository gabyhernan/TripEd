<%= form_for @trip do |f| %>


  <div class="field">
    <%= f.label :date %>
    <%= f.date_field :date %>
  </div>

  <div class="field">
    <%= f.label :start_time %>
    <%= f.text_field :start_time %>
  </div>

  <div class="field">
    <%= f.label :reserved %>
    <%= f.check_box :reserved %>
  </div>

  <div class="actions">
    <%= submit_tag "Submit", class:"waves-effect waves- btn" %>
  </div>


<% end %>
